{
    "Title": "Screening Packet Questionnaire",
    "Sections": {
        "Client Information": [          
            { "question_text": "What is your last name?", "question_type": "text" },
            { "question_text": "What is your first name?", "question_type": "text" },
            { "question_text": "What is your middle name?", "question_type": "text" },
            { "question_text": "What is your date of birth?", "question_type": "date" },
            { "question_text": "What is your address?", "question_type": "text" },
            { "question_text": "What is your phone number?", "question_type": "text" },
            { "question_text": "What is your cell number?", "question_type": "text" },
            { "question_text": "What is your email?", "question_type": "text" },
            { "question_text": "What is your sex?", "question_type": "multiple_choice",
                "options": ["Male", "Female"]},
            {
                "question_text": "What is your ethnicity?",
                "question_type": "multiple_choice",
                "options": ["White", "Hispanic", "Indian/Native American", "Black", "Other"]
            },
            {
                "question_text": "What is your marital status?",
                "question_type": "multiple_choice",
                "options": ["Married", "Single", "Separated", "Divorced", "Widowed"]
            },
            {
                "question_text": "What is your preferred language?",
                "question_type": "multiple_choice_with_other",
                "options": ["English", "Spanish", "Other"],
                "other_option": {
                    "enabled": true,
                    "field_name": "Other",
                    "field_type": "text",
                    "placeholder": "Please specify other behavioral health issues"
                }
            },
            {
                "question_text": "Do you require special accommodations due to a disability?",
                "question_type": "boolean"
            },
            {
                "question_text": "Are you court-ordered to attend this screening?",
                "question_type": "boolean"
            },
            { "question_text": "If yes, name the referring court or agency.", "question_type": "text" },
            { "question_text": "What is the charge or reason for referral?", "question_type": "text" },
            { "question_text": "What is your AZ Driver’s License/I.D. number?", "question_type": "text" },
            { "question_text": "What is the DUI violation date?", "question_type": "date" } ],  

        "Authorization of Release Information": [
            {
                "question_text": "Do you authorize ABC to communicate and exchange information about your case with:",
                "question_type": "multiple_data_entry",
                "fields": [
                    {
                        "field_name": "Court",
                        "field_type": "text",
                        "placeholder": "Enter the court name or details"
                    },
                    {
                        "field_name": "Agency",
                        "field_type": "text",
                        "placeholder": "Enter the agency name or details"
                    },
                    {
                        "field_name": "ADOT/MVD",
                        "field_type": "text",
                        "placeholder": "Enter any ADOT/MVD details"
                    },
                    {
                        "field_name": "Personal Advocate",
                        "field_type": "text",
                        "placeholder": "Enter the advocate's name and contact info"
                    },
                    {
                        "field_name": "Other",
                        "field_type": "text",
                        "placeholder": "Provide additional details"
                    }
                ]
            },
            {
                "question_text": "Who is your emergency contact?",
                "question_type": "text",
                "placeholder": "Enter the name of your emergency contact"
            },
            {
                "question_text": "What is the phone number of your emergency contact?",
                "question_type": "text",
                "placeholder": "Enter the phone number of your emergency contact"
            }
        ],
        "Screening Information": [
            {"question_text": "Client signature", "question_type": "text"},
            {"question_text": "Today's Date", "question_type": "date"}
        ],
        "Referal Agency List":[
            {"question_text": "Client signature", "question_type": "text"},
            {"question_text": "Today's Date", "question_type": "date"}
        ],
        "Intake / Assessment": [
            {
                "question_text": "What are your presenting behavioral health issues?",
                "question_type": "multiple_choice_with_other",
                "options": ["Substance Issue", "Domestic Violence", "Emotional/Behavioral Issue"],
                "other_option": {
                    "enabled": true,
                    "field_name": "Other",
                    "field_type": "text",
                    "placeholder": "Please specify other behavioral health issues"
                }
            },
            {
                "question_text": "Do you have a psychological history?",
                "question_type": "multiple_choice_with_other",
                "options": [
                    "Anxiety",
                    "Suicide",
                    "Gambling",
                    "Mental Illness",
                    "Depression",
                    "Adjustment",
                    "Eating Disorder"
                    
                ],
                "other_option": {
                    "enabled": true,
                    "field_name": "Other",
                    "field_type": "text",
                    "placeholder": "Please specify other issues"
                }
            },
            {
                "question_text": "Do you have any current or past family or personal issues?",
                "question_type": "multiple_choice_with_other",
                "options": [
                    "Abusive Relationships",
                    "Divorce",
                    "Family Issues",
                    "Childhood Abuse",
                    "Personal Issues"
                ],
                "other_option": {
                    "enabled": true,
                    "field_name": "Other",
                    "field_type": "text",
                    "placeholder": "Please specify other issues"
                }
            },
            {
                "question_text": "What is your educational and vocational history?",
                "question_type": "multiple_choice_with_other",
                "options": ["High School", "Tech School", "Community College", "College", "University"
                ],
                "other_option": {
                    "enabled": true,
                    "field_name": "Other",
                    "field_type": "text",
                    "placeholder": "Please specify other education history"
                }

            },
            {
                "question_text": "Do you have a medical history of any of the following?",
                "question_type": "multiple_choice_with_other",
                "options": [
                    "AIDS/HIV",
                    "Allergy",
                    "Asthma",
                    "Blood Pressure",
                    "Brain Trauma",
                    "Cancer",
                    "Diabetes",
                    "Emphysema",
                    "Epilepsy",
                    "Hearing Issues",
                    "Heart Disease",
                    "Kidney Disease",
                    "Liver Disease",
                    "STDs",
                    "Ulcers",
                    "Vision Issues"
                ],
                "other_option": {
                    "enabled": true,
                    "field_name": "Other",
                    "field_type": "text",
                    "placeholder": "Please specify other medical history"
                }
            },
            { "question_text": "If you take prescribed medications, enter it here", "question_type": "text" },
            { "question_text": "If you use over-the-counter medications, enter it here?", "question_type": "text" },
            {
                "Medication & Substance Use History": [
                    {
                        "question_text": "Indicate all substances used and experimented with:",
                        "question_type": "table",
                        "columns": [
                            { "header": "Substance", "type": "text" },
                            { "header": "Never Used", "type": "boolean" },
                            { "header": "Date Last Used (approximate)", "type": "date" },
                            { "header": "How Much Usually Used", "type": "multiple_choice", "options": ["1-3", "4-6", "7-12", "Over 12"] },
                            { "header": "How Often Usually Used", "type": "multiple_choice", "options": ["Yearly", "Monthly", "Weekly", "Daily"] }
                        ],
                        "rows": [
                            "Alcohol",
                            "Amphetamines",
                            "Antidepressants",
                            "Antipsychotics",
                            "Cocaine",
                            "Hallucinogens",
                            "Marijuana",
                            "Narcotics",
                            "Sedatives",
                            "Other"
                        ]
                    }
                ]
            },
            {
                "Legal History": [
                    {
                        "question_text": "Check all that apply, including your current situation:",
                        "question_type": "multiple_choice_with_dates",
                        "options": [
                            { "label": "DUI (alcohol or drug)", "date_fields": ["First Offense", "Second Offense", "Third Offense"] },
                            { "label": "Non-DUI (alcohol or drug)", "date_fields": ["First Offense", "Second Offense", "Third Offense"] },
                            { "label": "Domestic Violence or family disturbances (including those not resulting in arrest)",
                                "date_fields": ["First Offense", "Second Offense", "Third Offense"],
                                "additional_field": {
                                    "field_name": "Description",
                                    "field_type": "text",
                                    "placeholder": "Provide a description of the circumstances."
                                }
                            },
                            { "label": "Order of Protection (Restraining Order)", "date_fields": ["Date Issued"] },
                            { "label": "Non-D.V. Assault/Conflict", "date_fields": ["First Offense", "Second Offense", "Third Offense"] },
                            { "label": "Other Legal Issues (Diversion, Incarceration, Probation, Parole)", "date_fields": ["Dates"] }
                        ]
                    }
                ]
            },
            {
                "Behavioral Health Treatment History": [
                    {
                        "question_text": "Behavioral Health Treatment and Behavioral Health Hospitalization History:",
                        "question_type": "table",
                        "columns": [
                            { "header": "Type of Program", "type": "text" },
                            { "header": "Agency", "type": "text" },
                            { "header": "Purpose", "type": "text" },
                            { "header": "Date", "type": "date" },
                            { "header": "Assignment/Program", "type": "text" }
                        ],
                        "rows": [
                            "No Program Attended",
                            "Prior Screening Assessment",
                            "Education/DUI Class/Workshop",
                            "Outpatient Treatment",
                            "Inpatient Treatment"
                        ]
                    }    
                ]
            }
            
            
        ],
        "MAST": [
        {
            "question_text": "Do you enjoy a drink now and then?",
            "question_type": "boolean"
        },
        {
            "question_text": "Do you feel you are a normal drinker or drug user? ('Normal' means drinking or using less than or about the same as most other people and not getting into recurring trouble while drinking or using.)",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when you last felt you were a normal drinker or user"
                }
            ]
        },
        {
            "question_text": "Have you ever awakened after drinking or drug use and found that you could not remember a part of what happened?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Number of Times",
                    "field_type": "number",
                    "placeholder": "Enter the number of times"
                },
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when this occurred"
                }
            ]
        },
        {
            "question_text": "Does anyone worry or complain about your drinking or drug use?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Who Worries",
                    "field_type": "text",
                    "placeholder": "Enter the name of the person(s) who worries"
                }
            ]
        },
        {
            "question_text": "Can you stop drinking or drug use without a struggle after one or two drinks or uses?",
            "question_type": "boolean"
        },
        {
            "question_text": "Do you feel guilty about your drinking or drug use?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "How Often",
                    "field_type": "text",
                    "placeholder": "Describe how often you feel guilty"
                }
            ]
        },
        {
            "question_text": "Do friends or relatives think you are a normal drinker or drug user? ('Normal' means no problems caused by drinking or drug use.)",
            "question_type": "boolean"
        },
        {
            "question_text": "Are you able to stop drinking or drug use when you want to?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever attended a meeting of Alcoholics Anonymous or other 12-step meetings (e.g., AA, NA, CA)?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Why",
                    "field_type": "text",
                    "placeholder": "Explain why you attended"
                },
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when you attended"
                }
            ]
        },
        {
            "question_text": "Have you gotten into physical fights when you have been drinking or using drugs?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Number of Times",
                    "field_type": "number",
                    "placeholder": "Enter the number of times"
                },
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when this occurred"
                }
            ]
        },
        {
            "question_text": "Has your drinking or drug use ever created problems between you and your parents, another relative, spouse, girlfriend, or boyfriend?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Number of Times",
                    "field_type": "number",
                    "placeholder": "Enter the number of times"
                },
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when this occurred"
                }
            ]
        },
        {
            "question_text": "Has any family member ever sought help for your drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever lost friends because of your drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever gotten into trouble at work or school because of drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever lost your job because of drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever neglected your obligations (school, work, family) for two or more days in a row because you were drinking or using drugs?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Number of Times",
                    "field_type": "number",
                    "placeholder": "Enter the number of times"
                },
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when this occurred"
                }
            ]
        },
        {
            "question_text": "Do you drink or use drugs before noon fairly often?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever been told you have liver trouble, such as cirrhosis?",
            "question_type": "boolean"
        },
        {
            "question_text": "After heavy drinking or drug use, have you ever had severe shaking or heard voices/seen things that weren’t there?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever gone to someone (e.g., education or treatment) for help with your drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever been in a hospital because of drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever been hospitalized in a psychiatric ward where drinking or drug use contributed to the problem?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever sought help from a clinic, doctor, social worker, or clergy for an emotional problem involving drinking or drug use?",
            "question_type": "boolean"
        },
        {
            "question_text": "Have you ever been cited or arrested for DUI or other drug/alcohol-related offenses?",
            "question_type": "boolean_with_text",
            "additional_fields": [
                {
                    "field_name": "Number of Times",
                    "field_type": "number",
                    "placeholder": "Enter the number of times"
                },
                {
                    "field_name": "When",
                    "field_type": "text",
                    "placeholder": "Enter when this occurred"
                }
            ]
        }
    ],
        "ADHS": {
            "Part A": [
                {
                    "question_text": "Have you had 3 or more alcohol/drug-related arrests or citations?",
                    "question_type": "boolean"
                },
                {
                    "question_text": "Was your blood alcohol level 0.15 or above?",
                    "question_type": "boolean"
                },
                {
                    "question_text": "Do you feel you have a problem controlling alcohol or drugs?",
                    "question_type": "boolean"
                }
            ],
            "Part B": [
                {
                    "question_text": "Have you had 2 alcohol/drug-related arrests or citations?",
                    "question_type": "boolean"
                },
                {
                    "question_text": "Have you ever lost time from work or school due to alcohol or drug use?",
                    "question_type": "boolean"
                },
                {
                    "question_text": "Have you ever gone to a DUI class, Alcohol/Drug class, or treatment center?",
                    "question_type": "boolean"
                }
            ]
        },
        "A.B.I.": [
            { "question_text": "Do you get mad easily or have an explosive temper?", "question_type": "boolean" },
            { "question_text": "Do people tell you to calm down?", "question_type": "boolean" },
            { "question_text": "Has your anger caused problems for your family or friends?", "question_type": "boolean" }
        ],
        "T.V.I.": [
            {
                "question_text": "Have you witnessed or experienced a life-threatening or terrifying event?",
                "question_type": "boolean"
            },
            {
                "question_text": "Have you witnessed or experienced physical, emotional, or sexual abuse?",
                "question_type": "boolean"
            }
        ],
        "S.I.": [
            {
                "question_text": "Do you feel life has become very painful or unbearable?",
                "question_type": "boolean"
            },
            {
                "question_text": "Have you thought of hurting yourself or killing yourself?",
                "question_type": "boolean"
            }
        ],
        "D.I.": [
            {
                "question_text": "Do you feel very lonely, abandoned, or isolated?",
                "question_type": "boolean"
            },
            { "question_text": "Do you feel your life is a failure lately?", "question_type": "boolean" }
        ],
        "V.I.": [
            {
                "question_text": "The conflict that brought you here involved which of the following injuries?",
                "question_type": "multiple_choice",
                "options": [
                    "No Injury",
                    "Minor Injury (recovery in days)",
                    "Injury (recovery in weeks)",
                    "Injury (recovery in months)",
                    "Disabling Injury",
                    "Death"
                ]
            }
        ],
        "L.O.F.": [
            {
                "question_text": "Did the conflict involve arguing, physical contact, or weapon use?",
                "question_type": "multiple_choice",
                "options": [
                    "Arguing",
                    "Property Damage",
                    "Physical Contact",
                    "Physical Force",
                    "Weapon Display",
                    "Weapon Use"
                ]
            }
        ],
        "H.I.": [
            {
                "question_text": "Do you feel so hurt that you have the right to hurt someone?",
                "question_type": "boolean"
            },
            {
                "question_text": "Do you like to carry a weapon for protection or to feel powerful?",
                "question_type": "boolean"
            }
        ],
        "D.V.P.I.": [
            {
                "question_text": "Have you ever been cited, arrested, or charged for a domestic violence offense?",
                "question_type": "boolean"
            },
            {
                "question_text": "Do your conflicts follow a pattern of tension building, blow-ups, and peaceful times?",
                "question_type": "boolean"
            }
        ]
    }
}
